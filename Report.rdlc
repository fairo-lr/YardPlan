        <rd:TableName>DataTable1</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select 
DECODE(SUBSTR(YMC_MCHNO,2,2),'20','轮胎吊','21','轮胎吊','22','轮胎吊','23','轮胎吊','50','堆高机','51','正面吊') 机械类型,
YMC_MCHNO 编号,
RV_MEANING 机械状态,
YMC_CURLOC 当前位置,
YMC_CURDRIVER 当前司机工号,
USR_NAME 当前司机姓名,
YMA_OPDT 作业日期,
YMA_SHIFT 工班号
from PS_YARD_MACHINES ,PS_YARD_WORKING_MACHINES,PS_USERS,PS_REF_CODES
where YMC_MCHNO&lt;&gt;'S219'
and YMC_MCHNO&lt;&gt;'S220'
and YMC_MCHNO&lt;&gt;'S221'
and YMC_MCHNO&lt;&gt;'S222'
and YMC_MCHNO&lt;&gt;'S514'
and YMA_SHIFT=
(
select MAX(YMA_SHIFT) from PS_YARD_WORKING_MACHINES where
YMA_OPDT=
(select MAX(YMA_OPDT) from PS_YARD_WORKING_MACHINES )
)
and YMA_OPDT=
(select MAX(YMA_OPDT) from PS_YARD_WORKING_MACHINES )
and YMA_YMC_MCHNO=YMC_MCHNO
and YMC_CURDRIVER=USR_USERID
and RV_DOMAIN='机械状态' 
and  RV_LOW_VALUE=YMC_MCHSTATUS
order by YMC_MCHNO </CommandText>
        <DataSourceName>ConnectionString</DataSourceName>
      </Query>
